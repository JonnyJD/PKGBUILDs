From e90bb183074457195abb72b464500e65d31c5ed6 Mon Sep 17 00:00:00 2001
From: Logan Brown <thinkbrown@artemis.res.wpi.net>
Date: Thu, 7 Nov 2013 15:51:07 -0500
Subject: [PATCH] Replaced translate() with replace() to fix python 3.2
 compatibility

---
 rtslib/fabric.py | 4 ++--
 rtslib/utils.py  | 2 +-
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/rtslib/fabric.py b/rtslib/fabric.py
index 170ea3f..79ab972 100644
--- a/rtslib/fabric.py
+++ b/rtslib/fabric.py
@@ -368,7 +368,7 @@ def to_fabric_wwn(self, wwn):
     def from_fabric_wwn(self, wwn):
         if wwn.startswith("0x"):
             wwn = wwn[2:]
-        return "naa." + wwn.translate(None, ":")
+        return "naa." + wwn.replace(":", "")
 
     @property
     def wwns(self):
@@ -392,7 +392,7 @@ def to_fabric_wwn(self, wwn):
     def from_fabric_wwn(self, wwn):
         if wwn.startswith("0x"):
             wwn = wwn[2:]
-        return "ib." + wwn.translate(None, ":")
+        return "ib." + wwn.replace(":", "")
 
     @property
     def wwns(self):
diff --git a/rtslib/utils.py b/rtslib/utils.py
index 8f86d6d..5e5d9d2 100644
--- a/rtslib/utils.py
+++ b/rtslib/utils.py
@@ -300,7 +300,7 @@ def _cleanse_wwn(wwn_type, wwn):
     if wwn_type in ('naa', 'eui', 'ib'):
         if wwn.startswith("0x"):
             wwn = wwn[2:]
-        wwn = wwn.translate(None, ":-")
+        wwn = wwn.replace(":-", "")
 
         if not (wwn.startswith("naa.") or wwn.startswith("eui.") or \
             wwn.startswith("ib.")):
-- 
1.8.4

